{
   "parser": "@typescript-eslint/parser",
   "parserOptions": {
      "project": "./tsconfig.json"
   },
   "extends": [
      "eslint:recommended",
      "prettier",
      "standard",
      "plugin:@typescript-eslint/recommended"
   ],
   "plugins": [
      "@typescript-eslint"
   ],
   "env": {
      "node": true,
      "es6": true
   },
   "ignorePatterns": [
      "node_modules",
      "dist"
   ]
}